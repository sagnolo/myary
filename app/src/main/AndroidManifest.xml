<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nand.myary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/myary"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/myary"
        android:supportsRtl="true"
        android:theme="@style/Theme.Myary">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/google_ads_app_id"/>

        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.WriteActivity"/>
        <activity android:name=".ui.DiarysActivity"/>
    </application>

</manifest>